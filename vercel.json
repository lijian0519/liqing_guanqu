{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "PORT": "80",
    "CONFIG_NAME": "production",
    "DEBUG": "false",
    "DISABLE_SOCKETIO": "true",
    "MAX_TANKS": "11"
  },
  "regions": ["iad1"],
  "framework": null,
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "echo 'Building for Vercel deployment'"
}